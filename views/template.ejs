  <section class="disputa">
    <div class="disputa-wrapper">
      <div class="disputa-top">
        <p class="disputa-label">Times na disputa</p>
        <p class="responsavel-label">Respons√°vel</p>
      </div>

      <ul class="disputa-list">
        <% data?.times.forEach(function(timeAtual) { %>
          <li class="disputa-item">
            <p class="time-disputa">
              <%= timeAtual.nome %>
            </p>
            <p class="responsavel">
              <%= timeAtual.responsavel %>
            </p>
          </li>
          <% }); %>
      </ul>
    </div>
  </section>

  <section class="partidas-section">
    <h1 class="partidas-titulo">Partidas realizadas</h1>
    <ul class="partidas-list">
      <% data?.partidas.forEach(function(partidaAtual) { %>
        <li class="partida-item <% if (partidaAtual?.times[0].gols == partidaAtual?.times[1].gols) { %>draw<% } %>">
          <div class="left">
            <div class="item-content">
              <p class="item-time">
                <%= partidaAtual?.times[0].nome %>
              </p>
            </div>
            <div class="item-number-container">
              <p class="item-number <% if (partidaAtual?.times[0].gols > partidaAtual?.times[1].gols) { %>win<% } else { %>loose<% } %>">
                <%= partidaAtual?.times[0].gols %>
              </p>
            </div>
          </div>
          <div class="divisor"></div>
          <div class="right">
            <div class="item-number-container">
              <p class="item-number <% if (partidaAtual?.times[1].gols > partidaAtual?.times[0].gols) { %>win<% } else { %>loose<% } %>">
                <%= partidaAtual?.times[1].gols %>
              </p>
            </div>
            <div class="item-content">
              <p class="item-time">
                <%= partidaAtual?.times[1].nome %>
              </p>
            </div>
          </div>
        </li>
        <% }); %>
    </ul>
  </section>

